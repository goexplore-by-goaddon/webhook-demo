{
  "tasks": {
    "method": "get",
    "url": "http://0.0.0.0:3005/tasks/{{ _id }}",
    "headers": {
      "Authorization": "{{ token }}",
      "Content-Type": "application/json"
    }
  },
  "destroy_task": {
    "method": "delete",
    "url": "http://0.0.0.0:3005/tasks/{{ _id }}",
    "headers": {
      "Authorization": "{{ token }}",
      "Content-Type": "application/json"
    }
  }
}